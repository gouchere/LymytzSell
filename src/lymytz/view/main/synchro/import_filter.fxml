<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="300.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lymytz.service.application.synchro.impor.ImportFilterController">
   <children>
      <HBox spacing="10.0">
         <children>
            <VBox>
               <children>
                  <Label text="Colone" />
                  <ComboBox fx:id="CB_COL" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Valeur" />
                  <TextField fx:id="TXT_VAL" />
               </children>
            </VBox>
            <VBox layoutX="170.0" layoutY="10.0">
               <children>
                  <Label />
                  <Button mnemonicParsing="false" onAction="#openFilter" text="Ajouter" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="LIST_PARAM" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ListView>
      <Button fx:id="BTN_IMPORT" mnemonicParsing="false" onAction="#importer" text="Charger...">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
